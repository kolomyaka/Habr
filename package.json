        "cypress": "^11.0.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-boundaries": "^3.1.1",
        "eslint-plugin-i18next": "^5.1.2",
