{
    "name": "course",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve",
        "start:vite": "vite",
        "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\" \"npm run storybook\"",
        "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\" \"npm run storybook\"",
        "start:dev:server": "node ./json-server/index.js",
        "build:prod": "webpack --env mode=production",
        "build:dev": "webpack",
        "prettier": "npx prettier --write '**/*.{ts,tsx,json}'",
        "lint:ts": "eslint '**/*.{ts,tsx}'",
        "lint:ts:fix": "eslint '**/*.{ts,tsx}' --fix",
        "lint:scss": "npx stylelint '**/*.scss'",
        "lint:scss:fix": "npx stylelint '**/*.scss' --fix",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:unit:watch": "jest --config ./config/jest/jest.config.ts --watch",
        "test:e2e": "cypress open ",
        "test:ui": "loki test",
        "test:ui:ok": "loki approve",
        "test:ui:ci": "concurrently -s first -k \"npm run storybook\" \"npm run test:ui\"",
        "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
        "test:ui:json": "node ./scripts/generate-visual-json.js",
        "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "storybook": "storybook dev -p 6006 -c ./config/storybook",
        "storybook:build": "storybook build -c ./config/storybook",
        "postinstall": "node ./scripts/clear-cache.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "prettier --list-different",
            "eslint '**/*.{ts,tsx}' --fix"
        ],
        "*.scss": [
            "npx stylelint '**/*.scss' --fix"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/plugin-transform-typescript": "^7.18.12",
        "@babel/preset-env": "^7.21.4",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@floating-ui/react": "^0.23.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
        "@react-spring/web": "^9.7.2",
        "@storybook/addon-actions": "^7.0.20",
        "@storybook/addon-essentials": "^7.0.20",
        "@storybook/addon-interactions": "^7.0.20",
        "@storybook/addon-links": "^7.0.20",
        "@storybook/cli": "^7.0.20",
        "@storybook/react": "^7.0.20",
        "@storybook/react-webpack5": "^7.0.20",
        "@storybook/testing-library": "^0.1.0",
        "@svgr/webpack": "^6.2.1",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/circular-dependency-plugin": "^5.0.5",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@types/react": "^18.0.37",
        "@types/react-dom": "^18.0.11",
