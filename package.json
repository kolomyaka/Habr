        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@use-gesture/react": "^10.2.26",
        "@vitejs/plugin-react": "^4.0.0",
        "babel-loader": "^8.3.0",
        "babel-plugin-i18next-extract": "^0.8.2",
